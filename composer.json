{
    "name": "steinhaug/kista-ai",
    "description": "Less is more, more or less...",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Steinhaug\\KistaAi\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Kim Steinhaug",
            "email": "kim@steinhaug.com"
        }
    ],
    "require": {
        "orhanerday/open-ai": "^5.1",
        "php-http/discovery": "1.19.2",
        "psr/http-client-implementation": "*",
        "psr/http-factory-implementation": "*",
        "psr/http-client": "^1.0",
        "openai-php/client": "^0.8.4",
        "mmucklo/krumo": "^0.7.0",
        "steinhaug/sqlbuddy": "^1.3",
        "steinhaug/mysqli": "^1.6",
        "intervention/image": "^3.4",
        "matthiasmullie/minify": "^1.3"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "require-dev": {
        "symfony/http-client": "^6.4",
        "nyholm/psr7": "^1.8"
    }
